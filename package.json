{
  "name": "archive-viewer",
  "displayName": "Archive Explorer",
  "description": "A VSCode extension to explore any archive",
  "type": "commonjs",
  "version": "1.0.0",
  "publisher": "xiaobao",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/xiaobao520123/vscode-extension-archive-explorer"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:json",
    "onLanguage:jsonc"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": []
    },
    "views": {
      "explorer": [
        {
          "id": "archiveExplorer",
          "name": "Archive Explorer"
        }
      ]
    },
    "commands": [],
    "menus": {
      "commandPalette": [],
      "view/title": [],
      "view/item/context": []
    },
    "configuration": []
  },
  "scripts": {
    "vscode:prepublish": "webpack",
    "compile": "webpack",
    "watch": "webpack --watch"
  },
  "devDependencies": {
    "@types/node": "^18",
    "@types/vscode": "^1.73.0",
		"typescript": "^5.4.2"
  },
  "dependencies": {
    "copy-webpack-plugin": "^12.0.2",
    "libarchive.js": "^2.0.2"
  }
}
